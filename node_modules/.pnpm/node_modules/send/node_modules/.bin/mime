#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/MyProjectMandiri/Express/express-api-llat/node_modules/.pnpm/mime@1.4.1/node_modules/mime/node_modules:/mnt/c/MyProjectMandiri/Express/express-api-llat/node_modules/.pnpm/mime@1.4.1/node_modules:/mnt/c/MyProjectMandiri/Express/express-api-llat/node_modules/.pnpm/node_modules:/mnt/c/MyProjectMandiri/Express/express-api-llat/node_modules:/mnt/c/MyProjectMandiri/Express/node_modules:/mnt/c/MyProjectMandiri/node_modules:/mnt/c/node_modules:/mnt/c/MyProjectMandiri/Express/express-api-llat/node_modules/.pnpm/send@0.16.2/node_modules/send/node_modules:/mnt/c/MyProjectMandiri/Express/express-api-llat/node_modules/.pnpm/send@0.16.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/MyProjectMandiri/Express/express-api-llat/node_modules/.pnpm/mime@1.4.1/node_modules/mime/node_modules:/mnt/c/MyProjectMandiri/Express/express-api-llat/node_modules/.pnpm/mime@1.4.1/node_modules:/mnt/c/MyProjectMandiri/Express/express-api-llat/node_modules/.pnpm/node_modules:/mnt/c/MyProjectMandiri/Express/express-api-llat/node_modules:/mnt/c/MyProjectMandiri/Express/node_modules:/mnt/c/MyProjectMandiri/node_modules:/mnt/c/node_modules:/mnt/c/MyProjectMandiri/Express/express-api-llat/node_modules/.pnpm/send@0.16.2/node_modules/send/node_modules:/mnt/c/MyProjectMandiri/Express/express-api-llat/node_modules/.pnpm/send@0.16.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mime@1.4.1/node_modules/mime/cli.js" "$@"
else
  exec node  "$basedir/../../../../../mime@1.4.1/node_modules/mime/cli.js" "$@"
fi
