#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/MyProjectMandiri/Express/express-api-llat/node_modules/nopt/bin/node_modules:/mnt/c/MyProjectMandiri/Express/express-api-llat/node_modules/nopt/node_modules:/mnt/c/MyProjectMandiri/Express/express-api-llat/node_modules:/mnt/c/MyProjectMandiri/Express/node_modules:/mnt/c/MyProjectMandiri/node_modules:/mnt/c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/MyProjectMandiri/Express/express-api-llat/node_modules/nopt/bin/node_modules:/mnt/c/MyProjectMandiri/Express/express-api-llat/node_modules/nopt/node_modules:/mnt/c/MyProjectMandiri/Express/express-api-llat/node_modules:/mnt/c/MyProjectMandiri/Express/node_modules:/mnt/c/MyProjectMandiri/node_modules:/mnt/c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nopt/bin/nopt.js" "$@"
else
  exec node  "$basedir/../nopt/bin/nopt.js" "$@"
fi
